{
  "manifest_version": 2,

  "name": "SpeedyURLs",
  "icons":
   {
    "19": "icon.png",
    "128": "icon2.png"
   },
  "description": "This is a mass bookmark extension allowing you to open multiple bookmarks at once!",
  "version": "1.1",
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]

}